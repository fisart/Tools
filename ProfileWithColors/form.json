{
  "elements": [
    {
      "type": "ValidationTextBox",
      "name": "ProfileName",
      "caption": "Profile Name"
    },
    {
      "type": "NumberSpinner",
      "name": "StartValue",
      "caption": "Start Value",
      "digits": 2
    },
    {
      "type": "NumberSpinner",
      "name": "EndValue",
      "caption": "End Value",
      "digits": 2
    },
    {
      "type": "NumberSpinner",
      "name": "StepSize",
      "caption": "Step Size",
      "digits": 2
    },
    {
      "type": "SelectColor",
      "name": "StartColor",
      "caption": "Start Color"
    },
    {
      "type": "SelectColor",
      "name": "EndColor",
      "caption": "End Color"
    },
    {
      "type": "Select",
      "name": "VariableType",
      "caption": "Variable Type",
      "options": [
        { "label": "Integer", "value": "integer" },
        { "label": "Float", "value": "float" }
      ]
    },
    {
      "type": "Label",
      "name": "TruncateMessage",
      "caption": "Note: If you select the integer profile, any float values entered for Start Value, End Value, and Step Size will be truncated to integers."
    },
    {
      "type": "ValidationTextBox",
      "name": "Prefix",
      "caption": "Association Prefix"
    },
    {
      "type": "ValidationTextBox",
      "name": "Suffix",
      "caption": "Association Suffix"
    },
    {
      "type": "Button",
      "caption": "Create Profile",
      "onClick": "PWC_CreateProfileWithColors($id);"
    }
  ]
}
